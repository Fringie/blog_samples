{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "{tenant}.onmicrosoft.com",
    "TenantId": "{tenant-id}",
    "ClientId": "{client-id}",
    "ClientSecret":"{client-secret}",
    "CallbackPath": "/signin-oidc"
  },
  "SharePoint": {
    "Url": "https://{tenant}.sharepoint.com/sites/{site}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
